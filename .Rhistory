socjustsel$discr <- factor(socjustsel$discr, levels = c(1,2,3),
labels = c("Regularly", "Occasionally", "Never"),
ordered = T)
socjustsel$fear <- factor(socjustsel$fear, levels = c(1,2),
labels = c("Yes", "No"), ordered = T)
